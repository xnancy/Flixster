# Uncomment this line to define a global platform for your project
# platform :ios, '9.0'

target 'Flix' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'AFNetworking'
pod 'RateView'
pod 'MBProgressHUD'
  
target 'FlixTests' do
    inherit! :search_paths
    pod 'AFNetworking'
pod 'RateView'
  end

  target 'FlixUITests' do
    inherit! :search_paths
    pod 'AFNetworking'
pod 'RateView'
  end

end
